package lacture4;

public class Linearsearch {

	public static void main(String[] args) {
		int[] arr = {10,20,30,40,50};
		System.out.println(linearsearch(arr));
	}
	public static int linearsearch(int[] arr) {
		int temp=0;
		for(int i=0;i<=4;i++) {
			if(temp=arr[i]){
				temp=arr[i];
			}
		}
		return temp;

	}

}

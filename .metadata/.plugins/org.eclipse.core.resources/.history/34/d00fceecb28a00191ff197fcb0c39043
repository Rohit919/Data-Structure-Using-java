package lacture4;

public class Maxelement {

	public static void main(String[] args) {
		int[] arr = {10,50,30,40,20};
		System.out.println(max(arr));
	}
	public static int max(int[] arr) {
		int temp=;
		for(int i=0;i<=4;i++) {
			if(temp<arr[i]){
				temp=arr[i];
			}
		}
		return temp;
	}

}

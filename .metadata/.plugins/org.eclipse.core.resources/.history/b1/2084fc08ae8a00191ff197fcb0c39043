package lacture4;

public class Maxelement {

	public static void main(String[] args) {
		int[] arr = {10,20,30,40,50};
		System.out.println(max(arr));
	}
	public static int max(int[] arr) {
		for(int i=0;i<=5;i++) {
			int temp=0;
			if(temp<arr[i]){
				temp=arr[i];
			}
		}
		return temp;
	}

}
